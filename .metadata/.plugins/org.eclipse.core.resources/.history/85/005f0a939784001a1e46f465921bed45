package com.aeox.jkaiser.engine;

import java.util.List;

import org.springframework.stereotype.Service;

import com.aeox.jkaiser.dto.Job;
import com.aeox.jkaiser.dto.JobContext;
import com.aeox.jkaiser.dto.Result;
import com.aeox.jkaiser.exception.ParameterNotFoundException;

@Service
public class JobEngine {

	public List<Result<?>> run(final JobContext context, final Job job) throws ParameterNotFoundException {
		return job.run(context);
	}
	
}
